<h2>Novedades en Spots</h2>
<ul class="galeria">
<% var e; for(e=0; e<result.length; e++) { %>
	<li>
		<a href="#image<%= e %>"><img src="http://res.cloudinary.com/photo-spotkate/image/upload/<%= result[e].image %>"></a>
	</li>
<% } %>
</ul>
<% var e; for(e=0; e<result.length; e++) { %>
<div class="modal" id="image<%= e %>">
	<h3><a href="<%= result[e].spotId %>"><%= result[e].nombreSpot %></a></h3>
	<div class="imagen">
		<a href="#image<%= e-1 %>">&#60;</a>
		<a href="#image<%= e+1 %>"><img src="http://res.cloudinary.com/photo-spotkate/image/upload/<%= result[e].image %>"></a>
		<a href="#image<%= e+1 %>">></a>
	</div>
	<a class="cerrar" href="" >x</a>
	<h4> Foto subida por <%= result[e].creado_por %> el <%= result[e].creado_el %></h4>
</div>
<% } %>
